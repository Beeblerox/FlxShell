if (piped)
{
    return Shell.curDir.children;
}

else
{
    ret = "";

    for (x in Shell.curDir.children)
    {
        if (x.isDirectory)
        {
            ret += "[" + x.name + "]" + "\n";
        }
        else
        {
            ret += x.name + "\n";
        }
    }
    
    return ret.substr(0, ret.length - 1);
}